{
  "name": "polyvolve-admin",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "next -- -p 3000",
    "start:prod": "NODE_ENV=production next -- -p 3000",
    "build:dev": "next build",
    "build": "NODE_ENV=production next build",
    "export": "next export",
    "analyze": "NODE_ENV=production ANALYZE=true next build",
    "start": "next start -p $PORT",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "await-protect": "^1.0.11",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "formik": "^1.5.1",
    "isomorphic-fetch": "^2.2.1",
    "microtip": "^0.2.2",
    "moment": "^2.24.0",
    "next": "^9.4.2",
    "next-compose-plugins": "^2.1.1",
    "next-routes": "^1.4.2",
    "next-transpile-modules": "^3.3.0",
    "polyvolve-ui": "git+https://gitlab.com/polyvolve/ui.git",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-ga": "^2.5.3",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^4.2.0",
    "react-select": "^3.0.4",
    "react-toggle": "^4.0.2",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.9.1",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "webpack-bundle-analyzer": "^3.0.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.3.3",
    "@types/node": "^8.10.14",
    "@types/react": "^16.7.13",
    "@types/react-redux": "^6.0.0",
    "@types/styled-jsx": "^2.2.8",
    "babel-loader": "^8.0.4",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-transform-define": "^1.3.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "raw-loader": "^1.0.0",
    "sass": "^1.26.5",
    "typescript": "^3.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsConfig": "jest.tsconfig.json"
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "enzyme.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/enzyme.js",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/mocks.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/mocks.js"
    }
  }
}
